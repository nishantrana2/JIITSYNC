<div id="log">
<div >
  <img src="https://schoolbook.getmyuni.com/assets/images/rev_img/23570__4744/1476814587slide10[1].jpg">
</div>
<div>
<h2 class="one">Log in</h2>
 <p></p>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>
 <p></p>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>
   <p></p>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
   <p></p>
<% end %>

<%= render "devise/shared/links" %>
</div>
</div>